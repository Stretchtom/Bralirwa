<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.tmutabazi.schoolrecords.UserInterface.Quiz">

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="SID"
        android:id="@+id/textView4"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="67dp" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="QUIZ 1"
        android:id="@+id/textView5"
        android:layout_marginTop="78dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="QUIZ 2"
        android:id="@+id/textView6"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="61dp" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="QUIZ 3"
        android:id="@+id/textView7"
        android:layout_marginTop="48dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="QUIZ 4"
        android:id="@+id/textView8"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/textView7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="QUIZ 5"
        android:id="@+id/textView9"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/textView8" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/SID"
        android:layout_alignLeft="@+id/QUIZ2"
        android:layout_alignStart="@+id/QUIZ2"
        android:layout_above="@+id/textView5"
        android:layout_alignParentTop="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/QUIZ1"
        android:layout_alignTop="@+id/textView5"
        android:layout_toLeftOf="@+id/done"
        android:layout_toStartOf="@+id/done" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/QUIZ2"
        android:layout_alignBottom="@+id/textView6"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/QUIZ3"
        android:layout_above="@+id/textView8"
        android:layout_alignLeft="@+id/QUIZ2"
        android:layout_alignStart="@+id/QUIZ2" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/QUIZ4"
        android:layout_above="@+id/textView9"
        android:layout_alignLeft="@+id/QUIZ3"
        android:layout_alignStart="@+id/QUIZ3" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberSigned"
        android:ems="10"
        android:id="@+id/QUIZ5"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignLeft="@+id/QUIZ4"
        android:layout_alignStart="@+id/QUIZ4" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Done"
        android:id="@+id/done"
        android:layout_alignTop="@+id/textView9"
        android:layout_toRightOf="@+id/QUIZ5"
        android:layout_toEndOf="@+id/QUIZ5"
        android:layout_marginLeft="56dp"
        android:layout_marginStart="56dp" />
</RelativeLayout>
